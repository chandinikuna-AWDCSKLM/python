start=int(input("start value:"))
stop=int(input("end  value:"))
print("prime numbers between", start,"and", stop, "are:")
count= 0
sum=0
for n in range (start, stop+1):
 if n>1:
  for i in range (2,n):
   if n % i==0:
    break
  else:
      print(n);
      count+= 1;
      sum=sum+n;
print ( "primes are:",count)
print("sum of primes is:",sum)
